{
  "extends": "stylelint-config-pagarme-react",
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "each",
          "mixin",
          "define-mixin"
        ]
      }
    ],
    "property-no-unknown": [ true, {
        "ignoreProperties": ["composes"]
    }],
  }
}
